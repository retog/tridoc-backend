@prefix doc3:  <https://vocab.doc3.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

doc3:Tag a rdfs:Class .
doc3:ParametizableTag a rdfs:Class ;
    rdfs:commment "this is something that requires a parameter value to become an actual tag" .

doc3:label
   rdf:type rdf:Property ;
   rdfs:domain [
        owl:unionOf (doc3:ParametizableTag doc3:Tag)
    ];
   rdfs:range xsd:String.

doc3:value
   rdf:type rdf:Property ;
   rdfs:domain doc3:Tag .

doc3:valueType
   rdf:type rdf:Property ;
   rdfs:domain doc3:ParametizableTag .